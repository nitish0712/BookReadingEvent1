@using BookReadingEvent1.Models;
@using System.Collections.Generic;
@model IEnumerable<EventModel>
@{
    ViewBag.Title = "AllEvents";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2> All Events</h2>
<h2> Missed Events</h2>

<div class="container">
    @{
        <ul>
            @foreach (var item in Model)
            {
                <li>
                    @Html.ActionLink(@item.Title, "ViewEvent", "BookReadingEvent", new { @item.EventId }, null)
                </li>
            }
        </ul>
    }
</div>